generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String   // âœ… must be here
  username  String
  email     String   @unique
  password  String
  phone     String
  role      String
  createdAt DateTime @default(now())
}
